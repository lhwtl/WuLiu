<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hlp.mapper.AccBusinessadmissibilityMapper">
    <resultMap id="HlpAccBusinessadmissibilityAndAccWorkorder" type="AccBusinessadmissibility">
        <id property="id" column="id"></id>
        <result property="businessnoticeno" column="businessnoticeno"></result>
        <result property="reservationtime" column="reservationtime"></result>
        <result property="customname" column="customname"></result>
        <result property="pickupaddress" column="pickupaddress"></result>
        <result property="customcode" column="customcode"></result>
        <result property="linkman" column="linkman"></result>
        <result property="telphone" column="telphone"></result>
        <result property="weight" column="weight"></result>
        <result property="volume" column="volume"></result>
        <result property="importanthints" column="importanthints"></result>
        <result property="arrivecity" column="arrivecity"></result>
        <result property="pickerinfo" column="pickerinfo"></result>
        <result property="sendaddress" column="sendaddress"></result>
        <result property="processingunit" column="processingunit"></result>
        <result property="notificationsource" column="notificationsource"></result>
        <result property="customnomodifyflag" column="customnomodifyflag"></result>
        <result property="singletype" column="singletype"></result>
        <result property="packagesnum" column="packagesnum"></result>
        <result property="actualweight" column="actualweight"></result>
        <result property="billingweight" column="billingweight"></result>
        <result property="packingfee" column="packingfee"></result>
        <result property="actualpacking" column="actualpacking"></result>
        <collection property="accWorkorders" column="businessnoticeno" ofType="AccWorkorder">
         <id column="id" property="id"></id>
            <result property="jobno" column="jobno"></result>
            <result property="jobtype" column="jobtype"></result>
            <result property="pickupstatus" column="pickupstatus"></result>
            <result property="shortmessageint" column="shortmessageint"></result>
            <result property="workgenerationtime" column="workgenerationtime"></result>
            <result property="aftersinglenum" column="aftersinglenum"></result>
            <result property="smallmembernum" column="smallmembernum"></result>
            <result property="pickupunit" column="pickupunit"></result>
            <result property="pickuptime" column="pickuptime"></result>
            <result property="sortingcode" column="sortingcode"></result>
        </collection>
    </resultMap>
    <select id="selectHlpAccBusinessadmissibility"  resultMap="HlpAccBusinessadmissibilityAndAccWorkorder">
        select * from acc_businessadmissibility ab inner join acc_workorder aw on ab.businessnoticeno=aw.businessnoticeno
    </select>
</mapper>